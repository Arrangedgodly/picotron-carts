    x = 90,
    normal_sprite = ui[3].bmp,
    hover_sprite = ui[4].bmp
})

local playback = create_button_group(menuBar, {
    x = 5,
    y = 40
})

local playButton = create_hover_button(playback, {
    x = 40,
    normal_sprite = ui[8].bmp,
    hover_sprite = ui[7].bmp,
