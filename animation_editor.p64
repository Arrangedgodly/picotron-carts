		if self.hovered > 0 then
			spr(close_sprite)
		else
			spr(close_hover_sprite)
		end
	end,
	hover = function(self)
		self.hovered = 2
	end,
	click = function(self)
		popup_open = false
		reset_popup()
	end,
	update = function(self)
		if self.hovered > 0 then
			self.hovered -= 1
		end
	end
}

for h = 0, settings.height - 1 do
	for w = 0, settings.width - 1 do
		Window:attach{
			x = w * 32,
			y = h * 32,
			width = 32,
			height = 32,
			selected = false,
			frame_num = 0,
			hovered = 0,
			sprite_num = frame,
			draw = function(self)
				rectfill(0, 0, self.width, self.height, 0)
				sspr(self.sprite_num, 0, 0, 16, 16, 0, 0, 32, 32)
				
				if self.selected then
    				rect(0, 0, self.width-1, self.height-1, 7)
				end	
				
				if self.frame_num > 0 then
					print(self.frame_num, 2, 2, 7)
				end
			end,
			hover = function(self)
				self.hovered = 2
			end,
			update = function(self)
				if check_frames(self.sprite_num) > 0 and self.frame_num == 0 then
					self.frame_num = check_frames(self.sprite_num)
				end
				
				if self.hovered > 0 then
					self.hovered -= 1
					if Window.isDragging then
						self.selected = not self.selected
						if self.selected then
							table.insert(selected_frames, self.sprite_num)
						else
							table.remove(selected_frames)
							self.frame_num = 0
						end
					end
				end
			end
		}
		frame += 1
	end
end

function fillBG(self, col)
	rectfill(0,0,self.width,self.height,col)
end

function check_frames(frame_num)
	for i = 1, #selected_frames do
		if selected_frames[i] == frame_num then
			return i
		end
	end
	
	return 0
end

function reset_popup()
    selected_frames = {}
    frame = 1
    
    for child in all(Window.children) do
        if child.selected ~= nil then
            child.selected = false
            child.frame_num = 0
        end
    end
    
    Window.isDragging = false
end
