--[[pod_format="raw",author="Arranged Godly",created="2024-12-11 20:55:39",modified="2024-12-13 09:19:34",notes="A tool for creating afx\nanimation files from gfx files",revision=1548,title="Animation Editor",version="0.1"]]
include "gui.lua"
include "popup.lua"

popup_open = false

function _init()
  get_display()
  window({
    width = 480,
    height = 270,
    title = "temp.afx",
    has_frame = false,
    show_toolbar = true,
    tabbed = true,
    icon = userdata("[gfx]08087770777077707770777077700700070077777707777777777777777777777707[/gfx]"),
    x = 0,
    y = 0
  })
  
  mkdir("/ram/cart/afx")
  
end

function _update()
	gui:update_all()
	
	if popup_open then popup_gui:update_all() end
	
	if key("escape") then
		stop()
	end
end

function _draw()
	cls(theme"window_frame")
	gui:draw_all()
	
	if popup_open then popup_gui:draw_all() end
  
end
